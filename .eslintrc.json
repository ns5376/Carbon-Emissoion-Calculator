{
  
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
  
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 10,
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
    }
  }
  